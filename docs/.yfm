allowHTML: true
outputFormat: html

interface:
  toc-header: false
  favicon-src: https://i.postimg.cc/rw6SPB0d/Frame-9.jpg

search:
  provider: local
  tolerance: 2
  confidense: phrasedC

plugins:
  - "@diplodoc/search-extension"

docs-viewer:
  logo-options: 
   src: https://i.postimg.cc/rw6SPB0d/Frame-9.jpg
  themes: ['dark']


resources:
  style:
    - _assets/style/custom.css

extensions:
- name: github-vcs
- name: mdit-plugins
  plugins:
      - "markdown-it-ins"
      - name: markdown-it-emoji
        exportName: full

no-index: false

project-name: faq_transport_mos

authors:
  enabled: bool

mtimes: bool

contributors: bool