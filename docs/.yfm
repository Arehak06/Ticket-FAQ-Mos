allowHTML: true
outputFormat: html
no-index: false
mtimes: bool
project-name: faq_transport_mos

interface:
  toc-header: false
  favicon-src: https://i.postimg.cc/rw6SPB0d/Frame-9.jpg

search:
  provider: local
  tolerance: 2
  confidense: phrasedC

plugins:
  - "@diplodoc/search-extension"

docs-viewer:
  logo-options: 
   src: https://i.postimg.cc/rw6SPB0d/Frame-9.jpg
  themes: ['dark']

resources:
  style:
    - _assets/style/custom.css

extensions:
- name: github-vcs
- name: mdit-plugins
  plugins:
      - "markdown-it-ins"
      - name: markdown-it-emoji
        exportName: full

authors:
  enabled: bool

contributors:
  enabled: bool